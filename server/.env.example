# PostgreSQL connection
DATABASE_URL=postgresql://user:password@localhost:5432/inzynierka?schema=public
# JWT secrets
JWT_ACCESS_SECRET=replace_me_access
JWT_REFRESH_SECRET=replace_me_refresh
# Token expirations
JWT_ACCESS_EXP=15m
JWT_REFRESH_EXP=30d
# Port
PORT=4000
# Allegro/OLX OAuth placeholders
ALLEGRO_CLIENT_ID=your_allegro_client_id
ALLEGRO_CLIENT_SECRET=your_allegro_client_secret
OLX_CLIENT_ID=your_olx_client_id
OLX_CLIENT_SECRET=your_olx_client_secret

